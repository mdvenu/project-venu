{
    "version": 2,
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/(workout|food|index)",
            "destination": "/$1.html"
        },
        {
            "source": "/favicon.ico",
            "destination": "/favicon.svg"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}